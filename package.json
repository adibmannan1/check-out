{
  "name": "check-out",
  "version": "1.0.0",
  "description": "MERN stack eCommerce app",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adibmannan1/check-out.git"
  },
  "author": "MD ADIB MANNAN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adibmannan1/check-out/issues"
  },
  "homepage": "https://github.com/adibmannan1/check-out#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.3.4",
    "react-fade-in": "^2.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}
